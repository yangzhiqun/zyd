    text-decoration: none;
    border: 1px solid #999;
    padding: 5px 10px;
    position: relative;
    bottom: -1px;
  }

  ul.data-tabs a.active {
    border-bottom: 1px solid #FFFFFF;
  }

  .search-form th, .search-form td {
    font-size: 12px;
  }
