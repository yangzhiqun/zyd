        copied.find('input:eq(2)').attr({'value':data.bjpdata_1, 'title':data.bjpdata_1}).val(data.bjpdata_1);
        copied.find('input:eq(3)').attr({'value':data.bjpdata_2, 'title':data.bjpdata_2}).val(data.bjpdata_2);
        copied.find('input:eq(4)').attr({'value':data.bjpdata_3, 'title':data.bjpdata_3}).val(data.bjpdata_3);
        copied.find('input:eq(5)').attr({'value':data.bjpdata_4, 'title':data.bjpdata_4}).val(data.bjpdata_4);
