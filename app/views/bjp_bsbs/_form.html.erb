    // 按检测名称查找已记录的检测项目
    function find_bjp_data_by_name(name) {
        if(bjpdata == null) return null;
        var ret = null;
        $.each(bjpdata, function(i, data){
            if(data.bjpdata_0 == name) {
                ret = data;
                return false;
            }
        });
        return ret;
    }
    // 判断是否为数据库保存的数据
    function is_previous_data(){
        var current = $('#bjp_bsb_bjp_s_15').val() + $('#bjp_bsb_bjp_s_16').val();
        return current == bjpdata_condition;
    }



    // 渲染检查项目
    function render_spec_list(idx0, idx1) {
        $('.analysis-item').remove();
        var row = $('.analysis-item-templ').first().clone();
        row.removeClass('analysis-item-templ').addClass('analysis-item').find('input').removeAttr('value').val('');

        $.each(BJPJCX[idx0][idx1], function(index, data){
            var copied = row.clone();
            var o = find_bjp_data_by_name(data);
            copied.css('display', 'table-row');
            copied.find('span.idx').text(index + 1 + "、");
            copied.find('input:eq(1)').attr({'value':data, 'title':data}).val(data);
            $.each(BJPJCYJ[idx0][idx1][index], function(index,data){
                    copied.find('select').append("<option value='"+data+"'>"+data+"</option>");
                });
            copied.find('select').val(BJPJCYJ[idx0][idx1][index][0]);
            
            copied.find('input:eq(2)').attr('title', BJPPDYJ[idx0][idx1][index]).val(BJPPDYJ[idx0][idx1][index]);
            copied.find('input:eq(3)').attr('title', BJPJCDW[idx0][idx1][index]).val(BJPJCDW[idx0][idx1][index]);
            copied.insertBefore($('#result'));
        });
    }
